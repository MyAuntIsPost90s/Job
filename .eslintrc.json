{
  "env": {
    "browser": true,
    "commonjs": false,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "plugins": [
    "import"
  ],
  "rules": {
    "no-undef": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "no-unused-vars": "warn"
  },
  "globals": {
    "cc": true
  }
}
